{
  "name" : "ivoglent/elastic-apm-agent-v2",
  "description": "A php agent for Elastic APM",
  "license": "MIT",
  "version": "1.0.1",
  "require" : {
    "php" : ">= 7.1",
    "guzzlehttp/guzzle": "6.*",
    "ramsey/uuid": "^3.7",
    "ralouphie/getallheaders": "^2.0 || ^3.0",
    "ext-curl" : "*",
    "ext-json": "*"
  },
  "require-dev" : {
    "phpunit/phpunit" : "7.*",
    "justinrainbow/json-schema": "^5.2",
    "estahn/phpunit-json-assertions": "^3.0"
  },
  "suggest": {
    "ext-xdebug": "Required for processing of request headers"
  },
  "autoload" : {
    "psr-4" : {
      "Elastic\\Apm\\PhpAgent\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elastic\\Apm\\PhpAgent\\Tests\\": "tests/"
    }
  },
  "config" : {
    "optimize-autoloader" : true
  },
  "authors" : [
    {
      "name" : "Long Nguyen",
      "email" : "ivoglent@gmail.com",
      "homepage" : "http://longnv.info"
    }
  ]
}
